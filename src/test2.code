BEGIN CODE
    FOR (INT num = 1; num <= 100; num = num + 1)
    BEGIN FOR
        IF (num % 3 == 0 AND num % 5 == 0)
        BEGIN IF
            DISPLAY: "FizzBuzz"
        END IF

        ELSE IF (num % 3 == 0)
        BEGIN IF
            DISPLAY: "Fizz"
        END IF

        ELSE IF (num % 5 == 0)
        BEGIN IF
            DISPLAY: "Buzz"
        END IF

        ELSE
        BEGIN IF
            DISPLAY: num
        END IF
    END FOR
END CODE